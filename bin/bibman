BIBMAN_ROOT="$(cd `dirname $0`/.. && pwd)"
PYTHONPATH="${PYTHONPATH}:${BIBMAN_ROOT}/lib/python"

export PYTHONPATH

exec "${BIBMAN_ROOT}/lib/python/bibman/main.py" "$@"

